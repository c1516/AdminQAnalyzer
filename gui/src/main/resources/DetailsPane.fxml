<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<SplitPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <TableView fx:id="DetailTable" GridPane.columnIndex="0">
        <columns>
            <TableColumn prefWidth="150.0" text="Field Name">
                <cellValueFactory><PropertyValueFactory property="Name"/></cellValueFactory>
            </TableColumn>
            <TableColumn prefWidth="150.0" text="Field Value">
                <cellValueFactory><PropertyValueFactory property="Value"/></cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
    <TextArea fx:id="RawArea" />
</SplitPane>
