<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="Dialog" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <VBox>
            <children>
                <TextArea fx:id="TextBox" editable="false"
                          style="-fx-border-width: 0; -fx-background-color: transparent;"/>
                <HBox>
                    <children>
                        <HBox HBox.hgrow="ALWAYS"/>
                        <Button fx:id="CancelButton" alignment="CENTER" mnemonicParsing="false" onAction="#Cancel"
                                text="Cancel" textOverrun="CLIP">
                            <HBox.margin>
                                <Insets top="6.0"/>
                            </HBox.margin>
                        </Button>
                        <Button alignment="CENTER" mnemonicParsing="false" onAction="#Ok" text="OK" textOverrun="CLIP">
                            <HBox.margin>
                                <Insets left="5.0" right="5.0" top="6.0"/>
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
    <padding>
        <Insets top="5.0"/>
    </padding>
</AnchorPane>
